{
  "name": "identification-reader",
  "version": "1.0.0",
  "description": "Identification reader service",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "start:dev": "nodemon",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"*.{js,ts}\" \"{src,test}/**/*.{js,ts}\" "
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/dida/identification-reader.git"
  },
  "keywords": [
    "identification",
    "offline-identfication"
  ],
  "author": "kodirov.a@payme.uz",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/dida/identification-reader/issues"
  },
  "homepage": "https://bitbucket.org/dida/identification-reader#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "^17.0.33",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.24.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-extended": "^2.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.6.4",
    "yarn": "^1.22.18"
  },
  "dependencies": {
    "awilix": "^7.0.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "mongoose": "^6.8.1",
    "rand-token": "^1.0.1",
    "redis": "^4.1.0"
  }
}
